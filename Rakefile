require "bundler/gem_tasks"

desc "Default task"
task :helpful_output do
  puts "There are no Ruby tests in this project.\nTo run tests, navigate into spec/dummy/, then run 'rails server', and visit http://localhost:3000/specs to see results of Jasmine specs. \nThanks for using ng_will_paginate!"
end

task :default => ["helpful_output"]
